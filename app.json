{
  "name": "brayne-timesheet",
  "description": "Timesheet Management System built with Node.js and React",
  "repository": "https://github.com/braynedigi/Brayne-Timesheet-Management-System",
  "keywords": ["node", "express", "react", "timesheet", "postgresql"],
  "website": "https://github.com/braynedigi/Brayne-Timesheet-Management-System",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "NODE_ENV": {
      "description": "Environment setting",
      "value": "production"
    },
    "NPM_CONFIG_PRODUCTION": {
      "description": "Install production dependencies",
      "value": "false"
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
    "heroku-postgresql:mini"
  ]
}
